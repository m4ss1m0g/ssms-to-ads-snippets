{"Create Natively Compiled Stored Procedure":{
    "scope": "sql",
    "prefix": "ssmsCreateNativelyCompiledStoredProcedure",
    "body": [
        "--======================================================",
        "-- Create Natively Compiled Stored Procedure Template",
        "--======================================================",
        "",
        "-- Drop stored procedure if it already exists",
        "IF OBJECT_ID('${1:dbo}.${2:Procedure_Name}','P') IS NOT NULL",
        "   DROP PROCEDURE ${1:dbo}.${2:Procedure_Name}",
        "GO",
        "",
        "CREATE PROCEDURE ${1:dbo}.${2:Procedure_Name}",
        "\t-- Add the parameters for the stored procedure here",
        "\t${3:@p1} ${4:int} = ${5:0}, ",
        "\t${6:@p2} ${7:int} = ${8:0}",
        "WITH NATIVE_COMPILATION, SCHEMABINDING",
        "AS BEGIN ATOMIC WITH",
        "(",
        " TRANSACTION ISOLATION LEVEL = ${9:SNAPSHOT}, LANGUAGE = ${10:N'us_english'}",
        ")",
        "   --Insert statements for the stored procedure here",
        " SELECT ${3:@p1}, ${6:@p2}",
        "END",
        "GO",
        "",
        "-- =============================================",
        "-- Example to execute the stored procedure",
        "-- =============================================",
        "EXECUTE ${1:dbo}.${2:Procedure_Name} ${11:1}, ${12:2}",
        "GO"
    ],
    "description": "Create Natively Compiled Stored Procedure"
