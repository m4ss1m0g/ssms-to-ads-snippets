},"Create Procedure with CURSOR OUTPUT Parameter":{
    "scope": "sql",
    "prefix": "ssmsCreateProcedurewithCURSOROUTPUTParameter",
    "body": [
        "-- =======================================================",
        "-- Create stored procedure with CURSOR OUTPUT parameters ",
        "-- =======================================================",
        "",
        "-- Drop stored procedure if it already exists",
        "IF EXISTS (",
        "  SELECT * ",
        "    FROM INFORMATION_SCHEMA.ROUTINES ",
        "   WHERE SPECIFIC_SCHEMA = N'${1:Schema_Name}'",
        "     AND SPECIFIC_NAME = N'${2:Procedure_Name}' ",
        ")",
        "   DROP PROCEDURE ${1:Schema_Name}.${2:Procedure_Name}",
        "GO",
        "",
        "CREATE PROCEDURE ${1:Schema_Name}.${2:Procedure_Name}",
        "\t${3:@sample_procedure_cursor} CURSOR VARYING OUTPUT",
        "AS",
        "   SET ${3:@sample_procedure_cursor} = CURSOR FOR",
        "       ${4:select 1}",
        "   OPEN ${3:@sample_procedure_cursor}",
        "GO",
        "",
        "",
        "-- =============================================",
        "-- Example to execute the stored procedure",
        "-- =============================================",
        "DECLARE ${5:@test_cursor_variable} CURSOR",
        "",
        "EXEC ${1:Schema_Name}.${2:Procedure_Name} ${3:@sample_procedure_cursor} = ${5:@test_cursor_variable} OUTPUT",
        "",
        "WHILE (@@FETCH_STATUS = 0)",
        "BEGIN",
        "    FETCH NEXT FROM ${5:@test_cursor_variable}",
        "    PRINT 'put user defined code here'",
        "END",
        "",
        "CLOSE ${5:@test_cursor_variable}",
        "",
        "DEALLOCATE ${5:@test_cursor_variable}",
        "GO",
        ""
    ],
    "description": "Create Procedure with CURSOR OUTPUT Parameter"
},"Create Procedure with OUTPUT Parameter":{
    "scope": "sql",
    "prefix": "ssmsCreateProcedurewithOUTPUTParameter",
    "body": [
        "-- ===============================================",
        "-- Create stored procedure with OUTPUT parameters",
        "-- ===============================================",
        "-- Drop stored procedure if it already exists",
        "IF EXISTS (",
        "  SELECT * ",
        "    FROM INFORMATION_SCHEMA.ROUTINES ",
        "   WHERE SPECIFIC_SCHEMA = N'${1:Schema_Name}'",
        "     AND SPECIFIC_NAME = N'${2:Procedure_Name}' ",
        ")",
        "   DROP PROCEDURE ${1:Schema_Name}.${2:Procedure_Name}",
        "GO",
        "",
        "CREATE PROCEDURE ${1:Schema_Name}.${2:Procedure_Name}",
        "\t${3:@p1} ${4:int} = ${5:0}, ",
        "\t${6:@p2} ${7:int}  OUTPUT",
        "AS",
        "\tSELECT @p2 = @p2 + @p1",
        "GO",
        "",
        "-- =============================================",
        "-- Example to execute the stored procedure",
        "-- =============================================",
        "DECLARE ${8:@p2_output} ${9:int}",
        "",
        "EXECUTE ${1:Schema_Name}.${2:Procedure_Name} ${10:1}, ${8:@p2_output} OUTPUT",
        "",
        "SELECT ${8:@p2_output}",
        "GO",
        "",
        ""
    ],
    "description": "Create Procedure with OUTPUT Parameter"
